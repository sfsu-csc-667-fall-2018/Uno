{
  "name": "uno",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "SET NODE_ENV=development SET DEBUG=APP_NAME:* & nodemon ./bin/www",
